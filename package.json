{
	"name": "gql-to-sql",
	"description": "GQL to SQL transpiler",
	"version": "0.0.0",
	"type": "module",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"author": "vorant94",
	"license": "MIT",
	"keywords": ["gql", "sql", "transpiler", "compiler"],
	"repository": "https://github.com/vorant94/gql-to-sql",
	"homepage": "https://github.com/vorant94/gql-to-sql",
	"scripts": {
		"build": "tsc",
		"build:watch": "tsc --watch",
		"clean": "rm -rf dist",
		"prepare": "lefthook install",
		"lint:check": "biome check .",
		"lint:write": "biome check --write .",
		"lint:write:unsafe": "biome check --write --unsafe .",
		"lint:ci": "biome ci ."
	},
	"devDependencies": {
		"@biomejs/biome": "^1.8.3",
		"@total-typescript/ts-reset": "^0.5.1",
		"@types/node": "^22.3.0",
		"lefthook": "^1.7.12",
		"typescript": "^5.5.4"
	},
	"engines": {
		"node": ">=22"
	},
	"os": ["darwin", "linux"],
	"files": ["dist/*"]
}
